{
	"manifest_version": 2,
	"name": "DougDougGo",
	"version": "1.0",

	"description": "DougDougGo is now your new search engine",

	"icons": {
		"48": "assets/pepper.png"
	},

	"content_scripts": [
		{
			"css": ["doug.css"],
			"js": ["doug.js"],
			"run_at": "document_end",
			"matches": ["*://*.duckduckgo.com/*"]
		}
	]
}
